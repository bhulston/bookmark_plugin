{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/icon.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
      "background": {
        "service_worker": "background.js"
      },
      "permissions": ["activeTab", "storage", "scripting"] 
  }

